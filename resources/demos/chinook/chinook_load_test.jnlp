<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" codebase="http://jminor.no-ip.org/webstart/demos/" href="chinook_load_test.jnlp">

  <information>
    <title>Chinook Load Test JMinor Demo</title>
    <vendor>JMinor.org</vendor>
  </information>
  <security>
    <all-permissions/>
  </security>

  <resources>
    <j2se version="1.7+" href="http://java.sun.com/products/autodl/j2se" />

    <jar href="lib/jminor-demos.jar" main="true" />

    <!--  libraries-->
    <jar href="lib/jminor-client.jar"/>
    <jar href="lib/jminor-common.jar"/>
    <jar href="lib/jminor-plugins.jar"/>
    <jar href="lib/jcalendar-1.3.2.jar"/>
    <jar href="lib/slf4j-api-1.6.1.jar"/>
    <jar href="lib/logback-core-0.9.27.jar"/>
    <jar href="lib/logback-classic-0.9.27.jar"/>
    <jar href="lib/jasperreports-4.1.2.jar"/>
    <jar href="lib/jfreechart-1.0.13.jar"/>
    <jar href="lib/jcommon-1.0.15.jar"/>

    <property name="jminor.client.connectionType" value="remote"/>
    <property name="javax.net.ssl.trustStore" value="JMinorClientTruststore"/>
    <property name="jminor.server.hostname" value="jminor.no-ip.org" />
    <property name="jminor.report.path" value="reports/demos" />
    <property name="jminor.loadtest.thinktime" value="2500" />
    <property name="jminor.loadtest.logindelay" value="2" />
  </resources>

  <application-desc main-class="org.jminor.framework.demos.chinook.testing.ChinookLoadTest"/>
</jnlp>