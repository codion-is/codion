<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.jminor" module="jminor-common-ui"/>

  <configurations>
    <conf name="runtime"/>
    <conf name="sources"/>
    <conf name="javadoc"/>
  </configurations>

  <publications>
    <artifact name="jminor-common-ui" type="jar" ext="jar" conf="runtime"/>
    <artifact name="jminor-common-ui" type="pom" ext="pom" conf="runtime"/>
    <artifact name="jminor-common-ui-sources" type="source" ext="jar" conf="sources" m:classifier="sources"/>
    <artifact name="jminor-common-ui-javadoc" type="javadoc" ext="jar" conf="javadoc" m:classifier="javadoc"/>
  </publications>

  <dependencies defaultconf="runtime->default">
    <dependency org="org.jminor" name="jminor-common-model" rev="latest.integration"/>

    <dependency org="org.slf4j" name="slf4j-api" rev="1.7.7" transitive="false"/>
    <dependency org="ch.qos.logback" name="logback-classic" rev="1.1.2" transitive="false"/>
    <dependency org="ch.qos.logback" name="logback-core" rev="1.1.2" transitive="false"/>
    <dependency org="com.toedter" name="jcalendar" rev="1.4" transitive="false"/>
  </dependencies>
</ivy-module>
