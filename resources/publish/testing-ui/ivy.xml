<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.jminor" module="jminor-testing-ui"/>

  <configurations>
    <conf name="default"/>
    <conf name="master" extends="default"/>
    <conf name="sources"/>
    <conf name="javadoc"/>
  </configurations>

  <publications>
    <artifact name="jminor-testing-ui" type="jar" ext="jar" conf="default"/>
    <artifact name="jminor-testing-ui" type="pom" ext="pom" conf="default"/>
    <artifact name="jminor-testing-ui-sources" type="source" ext="jar" conf="sources" m:classifier="sources"/>
    <artifact name="jminor-testing-ui-javadoc" type="javadoc" ext="jar" conf="javadoc" m:classifier="javadoc"/>
  </publications>

  <dependencies>
    <dependency org="org.jminor" name="jminor-common-model" rev="latest.integration" conf="default"/>
    <dependency org="org.jminor" name="jminor-common-ui" rev="latest.integration" conf="default"/>
    <dependency org="org.jminor" name="jminor-db-core" rev="latest.integration" conf="default"/>
    <dependency org="org.jminor" name="jminor-db-local" rev="latest.integration" conf="default"/>
    <dependency org="org.jminor" name="jminor-client-model" rev="latest.integration" conf="default"/>
    <dependency org="org.jminor" name="jminor-client-ui" rev="latest.integration" conf="default"/>

    <dependency org="org.slf4j" name="slf4j-api" rev="1.7.7" transitive="false" conf="default"/>
    <dependency org="ch.qos.logback" name="logback-classic" rev="1.1.2" transitive="false" conf="default"/>
    <dependency org="ch.qos.logback" name="logback-core" rev="1.1.2" transitive="false" conf="default"/>
  </dependencies>
</ivy-module>
