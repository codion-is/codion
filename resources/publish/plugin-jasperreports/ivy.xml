<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.jminor" module="jminor-plugin-jasperreports"/>

  <configurations defaultconfmapping="runtime">
    <conf name="runtime"/>
    <conf name="sources"/>
    <conf name="javadoc"/>
  </configurations>

  <publications>
    <artifact name="jminor-plugin-jasperreports" type="jar" ext="jar" conf="runtime"/>
    <artifact name="jminor-plugin-jasperreports" type="pom" ext="pom" conf="runtime"/>
    <artifact name="jminor-plugin-jasperreports-sources" type="source" ext="jar" conf="sources" m:classifier="sources"/>
    <artifact name="jminor-plugin-jasperreports-javadoc" type="javadoc" ext="jar" conf="javadoc" m:classifier="javadoc"/>
  </publications>

  <dependencies defaultconf="runtime">
    <dependency org="org.jminor" name="jminor-common-model" rev="latest.integration"/>
    <dependency org="org.jminor" name="jminor-common-ui" rev="latest.integration"/>
    <dependency org="org.jminor" name="jminor-db-core" rev="latest.integration"/>

    <dependency org="org.slf4j" name="slf4j-api" rev="1.7.7"/>
    <dependency org="ch.qos.logback" name="logback-classic" rev="1.1.3"/>
    <dependency org="ch.qos.logback" name="logback-core" rev="1.1.3"/>

    <dependency org="net.sf.jasperreports" name="jasperreports" rev="6.0.0" transitive="false"/>
  </dependencies>
</ivy-module>
