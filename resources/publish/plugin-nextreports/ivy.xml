<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.jminor" module="jminor-plugin-nextreports"/>

  <configurations defaultconfmapping="runtime">
    <conf name="runtime"/>
    <conf name="sources"/>
    <conf name="javadoc"/>
  </configurations>

  <publications>
    <artifact name="jminor-plugin-nextreports" type="jar" ext="jar" conf="runtime"/>
    <artifact name="jminor-plugin-nextreports" type="pom" ext="pom" conf="runtime"/>
    <artifact name="jminor-plugin-nextreports-sources" type="source" ext="jar" conf="sources" m:classifier="sources"/>
    <artifact name="jminor-plugin-nextreports-javadoc" type="javadoc" ext="jar" conf="javadoc" m:classifier="javadoc"/>
  </publications>

  <dependencies defaultconf="runtime">
    <dependency org="org.jminor" name="jminor-common-model" rev="latest.integration"/>
    <dependency org="org.jminor" name="jminor-swing-common-model" rev="latest.integration"/>

    <dependency org="org.slf4j" name="slf4j-api" rev="1.7.7"/>
    <dependency org="ch.qos.logback" name="logback-classic" rev="1.1.3"/>
    <dependency org="ch.qos.logback" name="logback-core" rev="1.1.3"/>

    <dependency org="ro.nextreports" name="nextreports-engine" rev="8.2" transitive="false"/>
  </dependencies>
</ivy-module>
