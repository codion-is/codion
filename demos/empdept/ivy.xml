<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.jminor" module="jminor-demo-empdept"/>

  <configurations defaultconfmapping="compile">
    <conf name="compile"/>
    <conf name="runtime" extends="compile"/>
    <conf name="test" extends="runtime"/>
    <conf name="sources"/>
    <conf name="javadoc"/>
  </configurations>

  <dependencies defaultconf="compile->default">
    <dependency org="org.jminor" name="jminor-swing-client-bundle" rev="${version}"/>
    <dependency org="org.jminor" name="jminor-plugin-jasperreports" rev="${version}"/>
    <dependency org="org.jminor" name="jminor-plugin-json" rev="${version}"/>
    <dependency org="org.jminor" name="jminor-plugin-rest" rev="${version}"/>
    <dependency org="org.jminor" name="jminor-swing-client-test" rev="${version}"/>
    <dependency org="org.jminor" name="jminor-javafx" rev="${version}"/>

    <dependency org="javax.ws.rs" name="jsr311-api" rev="1.1.1" conf="compile->master"/>
    <dependency org="org.jminor" name="jminor-framework-db-test" rev="${version}" conf="compile->master"/>
    <dependency org="org.apache.httpcomponents" name="httpclient" rev="4.5" conf="compile->master"/>
    <dependency org="org.apache.httpcomponents" name="httpcore" rev="4.4.1" conf="compile->master"/>

    <dependency org="org.jfree" name="jcommon" rev="1.0.23" conf="compile->master"/>
    <dependency org="org.jfree" name="jfreechart" rev="1.0.19" conf="compile->master"/>

    <dependency org="net.sf.jasperreports" name="jasperreports" rev="6.0.0" conf="compile->master"/>
    <dependency org="commons-collections" name="commons-collections" rev="3.2.1" conf="runtime->master"/>
    <dependency org="commons-logging" name="commons-logging" rev="1.1.1" conf="runtime->master"/>
    <dependency org="commons-beanutils" name="commons-beanutils" rev="1.9.0" conf="runtime->master"/>
    <dependency org="commons-digester" name="commons-digester" rev="2.1" conf="runtime->master"/>
    <dependency org="org.eclipse.jdt.core.compiler" name="ecj" rev="4.3.1" conf="runtime->master"/>

    <dependency org="com.h2database" name="h2" rev="1.4.192" conf="runtime->master"/>

    <dependency org="org.jminor" name="jminor-swing-client-test" rev="${version}" conf="test->master"/>

    <dependency org="junit" name="junit" rev="4.12" conf="test->master"/>
    <dependency org="org.hamcrest" name="hamcrest-core" rev="1.3" conf="test->master"/>
  </dependencies>
</ivy-module>
